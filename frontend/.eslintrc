{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "@react-native-community"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@react-native-community"
  ],
  "rules": {
    "space-before-function-paren": [
      "warn",
      {
        "anonymous": "never",
        "asyncArrow": "always",
        "named": "never"
      }
    ]
  }
}
